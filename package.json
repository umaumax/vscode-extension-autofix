{
  "name": "vscode-extension-autofix",
  "displayName": "vscode-extension-autofix",
  "description": "Fix typo automatically",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.56.0"
  },
  "categories": ["Other"],
  "activationEvents": ["onCommand:type"],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [],
    "configuration": {
      "type": "object",
      "title": "Auto fix configuration",
      "properties": {
        "autofix.dictionary": {
          "type": "array",
          "default": {
            "languages": ["*"],
            "words": []
          },
          "description": "Auto fix definition list"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^7.1.3",
    "@types/mocha": "^8.2.2",
    "@types/node": "14.x",
    "@types/vscode": "^1.56.0",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "glob": "^7.1.7",
    "mocha": "^8.4.0",
    "typescript": "^4.3.2",
    "vscode-test": "^1.5.2"
  },
  "publisher": "umaumax",
  "repository": {
    "type": "git",
    "url": "https://github.com/umaumax/vscode-extension-autofix.git"
  }
}
